{
  "name": "Emvoutia",
  "description": "Ένα browser extension για τον εξευγενισμό ιστοσελίδων (https://sarantakos.wordpress.com/2021/05/27/but/)",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches":["<all_urls>"],
      "js":["background.js"]}
  ],
  "action": {
    "default_icon":
    {
      "16": "images/orange_16.png",
      "32": "images/orange_32.png",
      "128": "images/orange_128.png"
    }
  },
  "icons": {
    "16": "images/orange_16.png",
    "32": "images/orange_32.png",
    "128": "images/orange_128.png"
  }
}

